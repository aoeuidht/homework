#!/usr/bin/env python
# -*- coding: utf-8 -*-

"""
Given a string, find the length of the longest substring without repeating characters. For example, the longest substring without repeating letters for "abcabcbb" is "abc", which the length is 3. For "bbbbb" the longest substring is "b", with the length of 1.
"""

class Solution:
    # @return an integer
    def lengthOfLongestSubstring(self, s):
        if not s:
            return 0
        # the result pat
        r_head, r = 0, 1
        p_head, p = 0, 1
        slen = len(s)
        v = [-1] * 256

        v[ord(s[0])] = 0
        while p < slen:
            c = s[p]
            i = ord(c)
            if v[i] < 0:
                v[i] = p
            else:
                # conflict found
                p_len = p - p_head
                if p_len > r:
                    r_head = p_head
                    r = p_len
                # find the conflict position
                c_pos = v[i]
                for _i in range(p_head, c_pos+1, 1):
                    v[_i] = -1
                p_head = c_pos +  1
                v[i] = p
            p += 1
        p_len = p - p_head
        return p_len if p_len > r else r

if __name__ == '__main__':
    s = Solution()
